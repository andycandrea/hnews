<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Created At</th>
      <th>URL</th>
      <th>Content</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.created_at %></td>
        <td><%= article.url %></td>
        <td><%= article.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Article', new_article_path %>
